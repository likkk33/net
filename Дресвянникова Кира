#include <iostream>
   
   int main() {
       double num1, num2;

       std::cout << "2";
       std::cin >> num1;

       std::cout << "4";
       std::cin >> num2;

       double sum = num1 + num2;
       double product = num1 * num2;

       std::cout << "2" << num1 << std::endl;
       std::cout << "4" << num2 << std::endl;
       std::cout << "6" << sum << std::endl;
       std::cout << "8" << product << std::endl;
       std::cout << "2" << difference << std::endless;
       return 0;
   }
